        <custom-type translate="1">noo_property</custom-type>
        <custom-type translate="1">noo_membership</custom-type>
        <custom-type translate="0">noo_order</custom-type>
    </custom-types>    
    <taxonomies>
        <taxonomy translate="1">property_category</taxonomy>
        <taxonomy translate="1">property_status</taxonomy>
        <taxonomy translate="1">property_label</taxonomy>
        <taxonomy translate="1">property_location</taxonomy>
        <taxonomy translate="1">property_sub_location</taxonomy>
    </taxonomies>
    <admin-texts>
        <key name="noo_property_general" >
            <key name="area_unit" />
        </key>
    </admin-texts>
